{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc",
    "start": "node -r module-alias/register ./dist",
    "dev": "nodemon",
    "format": "prettier --write \"./src/**/*.{ts,tsx,json,css,scss,less}\" --config ./.prettierrc",
    "db:migration:generate": "sequelize migration:generate --migrations-path src/sequelize/migrations --name",
    "db:migrate": "npx sequelize-cli db:migrate --migrations-path src/sequelize/migrations",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo --migrations-path src/sequelize/migrations",
    "db:seed:generate": "sequelize seed:generate --seeders-path src/sequelize/seeders --name",
    "db:seed": "npm run db:build:migrations && sequelize db:seed:all"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, json",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/morgan": "^1.9.9",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "http": "^0.0.1-security",
    "http-status-codes": "^2.3.0",
    "i18next": "^23.7.6",
    "i18next-http-middleware": "^3.5.0",
    "i18next-node-fs-backend": "^2.1.3",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "rotating-file-stream": "^3.1.1",
    "sequelize": "^6.35.1",
    "sequelize-cli": "^6.6.2",
    "ts-custom-error": "^3.3.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "validatorjs": "^3.22.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "prettier": "^3.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "_moduleAliases": {
    "@/": "dist/",
    "@routers": "dist/routers",
    "@services": "dist/services",
    "@controllers": "dist/controllers",
    "@utils": "dist/utils",
    "@middlewares": "dist/middlewares",
    "@types": "dist/types",
    "@modules": "dist/modules",
    "@sequelize": "dist/sequelize"
  }
}
